{
    "name": "PBNJ",
    "description": "Capstone Project",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "./images/icons/PBNJ16.png",
        "32": "./images/icons/PBNJ32.png",
        "64": "./images/icons/PBNJ64.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_popup": "./popup.html",
        "default_icons": {
